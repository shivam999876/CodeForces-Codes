#include <stdio.h>Â int main(void){    int t;    scanf("%d", &t);    while (t--){        int n, c1 = 0, c2 = 0;        scanf ("%d", &n);        for (int i = 0; i < 3; i++){            c1 += (n % 10);            n = n / 10 ;        }        for (int i = 0; i < 3; i++){            c2 += (n % 10);            n = n / 10 ;        }        if (c1 == c2){            printf ("YES\n");        }        else{            printf("NO\n");        }    }}